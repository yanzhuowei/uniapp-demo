page {
  background-color: #dfe7e7;
}
.content image {
  width: 100%;
  height: 100%;
}
.banner {
  width: 100%;
  height: 340rpx;
}
.banner image {
  width: 100%;
  height: 340rpx;
}
.navs {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 30rpx 44rpx;
}
.navs image {
  width: 128rpx;
  height: 140rpx;
}
.floors .title {
  width: 750rpx;
  height: 60rpx;
  padding-top: 20rpx;
  background-color: #f4f4f4;
}
.floors .title image {
  width: 100%;
  height: 100%;
}
.floors .items {
  padding: 20rpx 16rpx;
  overflow: hidden;
}
.floors .items navigator {
  width: 232rpx;
  height: 188rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  float: left;
}
.floors .items navigator:nth-child(2n+1) {
  margin-right: 0;
}
.floors .items navigator:first-child {
  height: 386rpx;
  margin-right: 10rpx;
}

